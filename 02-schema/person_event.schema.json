
{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PersonEvent",
  "type": "object",
  "additionalProperties": false,
  "required": ["event_id", "name", "age", "ts_utc"],
  "properties": {
    "event_id": { "type": "integer", "minimum": 1 },
    "name": { "type": "string", "minLength": 1 },
    "age": { "type": "integer", "minimum": 0 },
    "ts_utc": { "type": "string" }
  }
}
